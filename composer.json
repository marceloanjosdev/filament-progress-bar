{
    "name": "marceloanjosdev/filament-progress-bar",
    "description": "A simple and easy to user Progress Bar",
    "keywords": [
        "marceloanjosdev",
        "laravel",
        "filament-progress-bar"
    ],
    "homepage": "https://github.com/marceloanjosdev/filament-progress-bar",
    "support": {
        "issues": "https://github.com/marceloanjosdev/filament-progress-bar/issues",
        "source": "https://github.com/marceloanjosdev/filament-progress-bar"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcelo Anjos",
            "email": "marcelo.e.anjos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0 || ^5.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/larastan": "^3.0",
        "orchestra/testbench": "^10.5",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MarceloAnjosDev\\FilamentProgressBar\\": "src/",
            "MarceloAnjosDev\\FilamentProgressBar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarceloAnjosDev\\FilamentProgressBar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarceloAnjosDev\\FilamentProgressBar\\FilamentProgressBarServiceProvider"
            ],
            "aliases": {
                "FilamentProgressBar": "MarceloAnjosDev\\FilamentProgressBar\\Facades\\FilamentProgressBar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
